# {{{ folder-hooks

folder-hook .                     source ~/.mutt/folders/DEFAULTS
folder-hook =lists/               source ~/.mutt/mailing-list
folder-hook =inboxes/             source ~/.mutt/mailing-list
folder-hook =inboxes/Mailbox$     source ~/.mutt/folders/Mailbox
folder-hook =(inboxes|lists)/zsh-            source ~/.mutt/folders/zsh
folder-hook =(inboxes|lists)/alsa-           source ~/.mutt/folders/alsa
folder-hook =(inboxes|lists)/void            source ~/.mutt/folders/void
folder-hook =(inboxes|lists)/linux-thinkpad  source ~/.mutt/folders/linux-thinkpad
folder-hook =(inboxes|lists)/linux-laptop    source ~/.mutt/folders/linux-laptop 
folder-hook =(inboxes|lists)/linux-diald    source ~/.mutt/folders/linux-diald
folder-hook =(inboxes|lists)/london-pm      source ~/.mutt/folders/london-pm

# }}}

